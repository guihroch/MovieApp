<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Background_text_input_layout">
        <item name="android:layout_marginHorizontal" tools:targetApi="o">20dp</item>
        <item name="hintEnabled">false</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>

    </style>

    <style name="Background_text_input_edit_text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="Button_geral">
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginTop">100dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:letterSpacing">0.2</item>
        <item name="textAllCaps">false</item>
    </style>




</resources>